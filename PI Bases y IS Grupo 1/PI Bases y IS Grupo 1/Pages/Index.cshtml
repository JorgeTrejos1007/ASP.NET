@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<!DOCTYPE html>
<html lang="es">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <link rel="icon" href="img/favicon.png" type="image/png" />
    <title>Comunidad practica</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="css/bootstrap.css" />
    <link rel="stylesheet" href="css/flaticon.css" />
    <link rel="stylesheet" href="css/themify-icons.css" />
    <link rel="stylesheet" href="vendors/owl-carousel/owl.carousel.min.css" />
    <link rel="stylesheet" href="vendors/nice-select/css/nice-select.css" />
    <!-- main css -->
    <link rel="stylesheet" href="css/style.css" />
</head>

<body>
    <!--================ Start Header Menu Area =================-->
    <header class="header_area">
        <div class="main_menu">
            <div class="search_input" id="search_input_box">
                <div class="container">
                    <form class="d-flex justify-content-between" method="get" action="">
                        <input type="text"
                               class="form-control"
                               id="search_input"
                               placeholder="Search Here" />
                        <button type="submit" class="btn"></button>
                        <span class="ti-close"
                              id="close_search"
                              title="Close Search"></span>
                    </form>
                </div>
            </div>

            <nav class="navbar navbar-expand-lg navbar-light">
                <div class="container">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <a class="navbar-brand logo_h" href="index.html">
                        <img src="img/logo.png" alt="" />
                    </a>
                    <button class="navbar-toggler"
                            type="button"
                            data-toggle="collapse"
                            data-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent"
                            aria-expanded="false"
                            aria-label="Toggle navigation">
                        <span class="icon-bar"></span> <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse offset"
                         id="navbarSupportedContent">
                        <ul class="nav navbar-nav menu_nav ml-auto">
                            <li class="nav-item">
                                <a class="nav-link" href="Miembros/DesplegarMiembros">Miembros</a>
                            </li>
                            <li class="nav-item submenu dropdown">
                                <a href="#"
                                   class="nav-link dropdown-toggle"
                                   data-toggle="dropdown"
                                   role="button"
                                   aria-haspopup="true"
                                   aria-expanded="false">Cursos</a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item">
                                        <a class="nav-link" href="courses.html">Cursos ofrecidos</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="course-details.html">Inscribirme</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="Curso/AprobarCurso">Cursos Propuestos</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="Curso/ProponerCurso">Proponer curso</a>
                                    </li>
                                </ul>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" href="index.html">Encuestas</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="index.html">Mi perfil</a>

                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="index.html">Iniciar sesión</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="Miembros/CrearMiembro">Registrarme</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" href="Catalogo/VerCatalogo">Catalago</a>
                            </li>


                            <li class="nav-item">
                                <a href="#" class="nav-link search" id="search">
                                    <i class="ti-search"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <!--================ End Header Menu Area =================-->
    <!--================ Start Home Banner Area =================-->
    <section class="home_banner_area">
        <div class="banner_inner">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="banner_content text-center">
                            <p class="text-uppercase">
                                Compartir conocimiento es lo más importante
                            </p>
                            <h2 class="text-uppercase mt-4 mb-5">
                                Comunidad práctica de aprendizaje
                            </h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--================ End Home Banner Area =================-->
    <!--================ Start Feature Area =================-->
    <section class="feature_area section_gap_top">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <div class="main_title">
                        <h2 class="mb-3">Nunca dejes de aprender</h2>
                        <p>
                            Somos una comunidad basada en compartir conocimiento
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4 col-md-6">
                    <div class="single_feature">
                        <div class="icon"><span class="flaticon-student"></span></div>
                        <div class="desc">
                            <h4 class="mt-3 mb-2">Misión</h4>
                            <p>
                                Compartir ideas, encontrar soluciones e innovar, uniendo sus esfuerzos para el desarrollo continuo
                                de un área de conocimiento especializado
                            </p>
                        </div>
                    </div>
                </div>

                <div class="col-lg-4 col-md-6">
                    <div class="single_feature">
                        <div class="icon"><span class="flaticon-book"></span></div>
                        <div class="desc">
                            <h4 class="mt-3 mb-2">Visión</h4>
                            <p>
                                Ser un espacio de formación continua, establecimiento de alianzas entre países miembros y generación de nuevo conocimiento.
                            </p>
                        </div>
                    </div>
                </div>

                <div class="col-lg-4 col-md-6">
                    <div class="single_feature">
                        <div class="icon"><span class="flaticon-earth"></span></div>
                        <div class="desc">
                            <h4 class="mt-3 mb-2">Países miembros</h4>
                            <p>
                                Entre nuestros Paises Miembros actualmente estan<br/>
                                @foreach (var miembro in (List<string>)ViewData["PaisesMiembro"])
                                {
                                    <li>@miembro</li>
                                }

                            </p>
                        </div>

                        <div class="desc">
                            <h4 class="mt-3 mb-2">¡Ya somos @Html.Label("", ViewData["TotalMiembros"].ToString()) miembros!</h4>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--================ End Feature Area =================-->
    <!--================ Start NOTICIAS =================-->
    <div class="popular_courses">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-5">
                    <div class="main_title">
                        <h2 class="mb-3">Noticias destacadas</h2>
                        <p>
                            Lo que acontece en la comunidad
                        </p>
                    </div>
                </div>
            </div>
            <div class="row">
                <!-- single course -->
                <div class="col-lg-12">
                    <div class="owl-carousel active_course">
                        <div class="single_course">
                            <div class="course_head">
                                <img class="img-fluid" src="img/courses/c1.jpg" alt="" />
                            </div>
                            <div class="course_content">
                                <!---- <span class="price">$25</span> -->
                                <h4 class="mb-3">
                                    <a href="course-details.html">¡Nuevo curso! Comunicación acertiva</a>
                                </h4>
                                <p>
                                    Orientado a mejorar las habilidades de comunicación.
                                    Todos los jueves de 7:00 p.m a 10:00 p.m
                                </p>
                            </div>
                        </div>

                        <div class="single_course">
                            <div class="course_head">
                                <img class="img-fluid" src="img/courses/c2.jpg" alt="" />
                            </div>
                            <div class="course_content">
                                <!---- <span class="price">$25</span> -->

                                <h4 class="mb-3">
                                    <a href="course-details.html">Salud mental en la pandemia</a>
                                </h4>
                                <p>
                                    Es importante que siga estos consejos para lidiar con esta nueva "normalidad"
                                </p>

                            </div>
                        </div>

                        <div class="single_course">
                            <div class="course_head">
                                <img class="img-fluid" src="img/courses/c3.jpg" alt="" />
                            </div>
                            <div class="course_content">
                                <!---- <span class="price">$25</span> -->

                                <h4 class="mb-3">
                                    <a href="course-details.html">Recursos para educación en latinoamérica se reducirán un 9%</a>
                                </h4>
                                <p>
                                    La Organización de las Naciones Unidas instó a los gobiernos latinoamericanos a salvaguardar y priorizar los recursos de la educación.
                                </p>


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--================ End Popular Courses Area =================-->

    <!--================ Start Trainers Area =================-->
